-    set(CMAKE_SHARED_LINKER_FLAGS -Wl,-rpath=$ORIGIN:$ORIGIN/../${TENSORRT_MODULE}_libs)
-endif()
-
 # -------- PATHS --------
 message(STATUS "EXT_PATH: ${EXT_PATH}")
 message(STATUS "TENSORRT_BUILD: ${TENSORRT_BUILD}")
